&lt;/span&gt;&lt;/span&gt;&lt;span class="line"&gt;&lt;span class="cl"&gt;D:&lt;span class="se"&gt;\S&lt;/span&gt;trawberry&lt;span class="se"&gt;\p&lt;/span&gt;erl&lt;span class="se"&gt;\s&lt;/span&gt;ite&lt;span class="se"&gt;\b&lt;/span&gt;in
&lt;/span&gt;&lt;/span&gt;&lt;span class="line"&gt;&lt;span class="cl"&gt;D:&lt;span class="se"&gt;\S&lt;/span&gt;trawberry&lt;span class="se"&gt;\p&lt;/span&gt;erl&lt;span class="se"&gt;\b&lt;/span&gt;in&lt;/span&gt;&lt;/span&gt;&lt;/code&gt;&lt;/pre&gt;&lt;/td&gt;&lt;/tr&gt;&lt;/table&gt;
